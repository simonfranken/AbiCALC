<Page x:Class="AbiCALC.Pages.mainWindow.page_add"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AbiCALC.Pages.mainWindow"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="NOTE HINZUFÜGEN"
      xmlns:lib.AListSelection="clr-namespace:lib.AListSelection;assembly=lib"
      xmlns:lib.ATextBox="clr-namespace:lib.ATextBox;assembly=lib"
      Style="{StaticResource pageMain}">

    <Grid>


        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <lib.AListSelection:AListSelection Grid.Column="0"  x:Name="subjectSelection" Margin="2.5"/>
        <lib.AListSelection:AListSelection Grid.Column="1" x:Name="semesterSelection" Margin="2.5"/>

        <lib.ATextBox:ATextBox Grid.Column="2"></lib.ATextBox:ATextBox>
    </Grid>
</Page>
